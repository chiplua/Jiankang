<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@*android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#ffd9d9d9" android:endColor="#ff9a9a9a" android:angle="270.0" android:centerY="0.75" android:centerColor="#ffcccccc" />
        </shape>
    </item>
    <item android:id="@*android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#80ade32b" android:endColor="#a0679415" android:angle="270.0" android:centerY="0.75" android:centerColor="#8087b81f" />
            </shape>
        </clip>
    </item>
    <item android:id="@*android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ff9dd93d" android:endColor="#ff509a00" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff71ca10" />
            </shape>
        </clip>
    </item>
</layer-list>